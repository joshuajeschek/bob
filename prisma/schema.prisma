generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String    @id @unique
  battleTag    String?   @map("battle_tag")
  platform     Platform?
  region       Region?
  connectToken String?   @unique
}

enum Platform {
  pc
  xbl
  psn
}

enum Region {
  us
  eu
  kr
  cn
  global
}
